/* Admin Layout */
.adminContentPane{
    background: url("");
}

.administrationContainer,.administrationSuccess,.administrationError{
    width: 80%;
    margin-left: auto;
    margin-right: auto;
    margin-top: 20px;
}

.administrationContainer>div{
    float: left;
}

.administrationContainer>.administrationTreeMenue{
    width: 25%;
    border: 1px solid black;
    -webkit-box-sizing: border-box;  /*issue with IE7*/
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.administrationContainer>.administrationTreeMenue ul{
    list-style: none;
    margin-top: 0px;
    padding-left: 0px;
    margin-bottom: 0px;
}

.administrationContainer>.administrationTreeMenue>ul>li>a	{
    padding-left:10px;
    background-color: rgba(0,0,0,0.8);
}

.administrationContainer>.administrationTreeMenue ul a	{
    display: block;
    height: 35px;
    padding-top:10px;
    background-color: rgba(0,0,0,0.5);
    text-decoration: none;
    color: white;
}

.administrationContainer>.administrationTreeMenue ul ul a	{
    padding-left:30px;
}

.administrationContainer>.administrationTreeMenue ul ul a:hover	{
    background-color: rgba(0,0,0,0.7);
}

.administrationContainer>.administrationTreeMenue ul ul{
    margin-left: 0px;
}



.administrationContainer>.adminstrationPanel{
    margin-left: 1%;
    width: 73.5%;
    border: 1px solid black;
    padding-left: 20px;
    -webkit-box-sizing: border-box;  /*issue with IE7*/
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding-top: 10px;
}


.administrationContainer>.adminstrationPanel>.createUserForm>.header{
    font-family: verdana, helvetica, arial, sans-serif;
    font-size: 30px;
    font-weight: bold;
}

.administrationContainer>.adminstrationPanel>.createUserForm>.header>p,
.administrationContainer>.adminstrationPanel>.createUserForm>.subheader>p{
    margin-top: 0px;
    margin-bottom: 0px;
}

.administrationContainer>.adminstrationPanel>.createUserForm>.subheader{
    font-family: verdana, helvetica, arial, sans-serif;
    font-size: 20px;
    font-style: italic;
}


.administrationContainer>.adminstrationPanel>.createUserForm>.inputs>.line>div{
    float: left;
}

.administrationContainer>.adminstrationPanel>.createUserForm>.inputs>.line>.label{
    clear: left;
    width:20%;
}
.administrationContainer>.adminstrationPanel>.createUserForm>.inputs>.line>.separator{
    width:5%;
}
.administrationContainer>.adminstrationPanel>.createUserForm>.inputs>.line>.input{
